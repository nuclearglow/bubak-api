{
    "name": "bubak-api",
    "version": "1.0.0",
    "description": "Bubak REST API using Express, Mongoose, and MongoDB.",
    "scripts": {
        "build:server:once": "cross-env NODE_ENV=development webpack --config webpack.config.js",
        "watch:server": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
        "nodemon:dev": "cross-env NODE_ENV=development nodemon --inspect ./dist/server.js",
        "start": "npm run build:server:once && npm-run-all --parallel nodemon:dev watch:server"
    },
    "author": "nuclearglow",
    "license": "anarchy",
    "keywords": [
        "BuBaK",
        "API",
        "Express",
        "REST",
        "Mongoose",
        "MongoDB"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:nuclearglow/bubak-api.git"
    },
    "bugs": {
        "url": "https://github.com/nuclearglow/bubak-api/issues"
    },
    "homepage": "https://github.com/nuclearglow/bubak-api#readme",
    "dependencies": {
        "body-parser": "^1.18.3",
        "chalk": "^2.4.1",
        "compression": "^1.7.1",
        "cors": "^2.8.4",
        "dotenv": "^5.0.0",
        "express": "^4.16.2",
        "express-unless": "^0.5.0",
        "formidable": "^1.2.1",
        "jsonwebtoken": "^8.3.0",
        "moment": "^2.22.2",
        "mongoose": "^5.3.4",
        "mongoose-bcrypt": "^1.5.0",
        "mongoose-string-query": "^0.2.5",
        "mongoose-timestamp": "^0.6.0",
        "nodemailer": "^4.6.8",
        "serve-favicon": "^2.4.5",
        "slug": "^0.9.2",
        "validator": "^9.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.2.0",
        "eslint": "^5.6.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "nodemon": "^1.18.5",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.10.2",
        "prettierrc": "^0.0.0-5",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9",
        "webpack-node-externals": "^1.7.2"
    }
}
